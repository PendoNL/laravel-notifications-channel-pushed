{
    "name": "pendonl/laravel-notifications-channel-pushed",
    "description": "Laravel 5.3+ Notifications Pushed Driver",
    "homepage": "https://github.com/pendonl/laravel-notifications-channel-pushed",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua de Gier",
            "email": "joshua@pendo.nl",
            "homepage": "https://pendo.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/notifications": "~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/support": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "PendoNL\\LaravelNotificationsChannelPushed\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PendoNL\\LaravelNotificationsChannelPushed\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "PendoNL\\LaravelNotificationsChannelPushed\\PushedServiceProvider"
            ]
        }
    }
}
